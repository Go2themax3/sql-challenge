CREATE TABLE departments (
   dept_no varchar(10),
   dept_name varchar(10)
);

CREATE TABLE dept_emp (
   emp_no int,
   dept_no varchar(4)
);

CREATE TABLE dept_manager (
	dept_no varchar (4),
	emp_no int
);

CREATE TABLE employees (
   emp_no int,
   emp_title_id varchar(10),
   birth_date date,
   first_name varchar(14),
   last_name varchar(16),
   sex varchar(1),
   hire_date date
);

CREATE TABLE salaries (
	emp_no int,
	salary int
);

CREATE TABLE titles (
	title_id varchar(5),
	title varchar(20)
);

